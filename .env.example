# Path to the saved LinkedIn session cookies file
LINKEDIN_COOKIE_PATH=./data/session_cookies.json

# Set to false to watch browsers during scraping (useful for debugging)
LINKEDIN_HEADLESS=true

# Optional: LinkedIn credentials for automated login.
# If omitted, a headed browser will open for manual login.
LINKEDIN_USERNAME=
LINKEDIN_PASSWORD=

# Seconds to wait for manual login completion (default: 120)
LINKEDIN_LOGIN_TIMEOUT_SECONDS=120
